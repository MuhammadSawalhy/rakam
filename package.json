{
  "name": "rakam",
  "version": "1.0.0-2",
  "description": "Math library focusing on the performance ðŸš€.",
  "main": "main",
  "module": "module",
  "files": [
    "main",
    "module"
  ],
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scicave/rakam.git"
  },
  "bugs": {
    "url": "https://github.com/scicave/rakam/issues"
  },
  "homepage": "https://github.com/scicave/rakam#readme",
  "scripts": {
    "start": "npx gulp cjs-watch",
    "build": "gulp build:cjs",
    "format": "prettier --write {src,tests}/**/*.js",
    "lint": "eslint src/ --fix",
    "test": "jest tests/",
    "test:watch": "npm t -- --watch",
    "test:cov": "npm t -- --collectCoverage && eval `cat .env` codecov",
    "test:ci": "npm t -- --ci --detectOpenHandles --forceExit",
    "preversion": "npm run format && npm run lint",
    "postversion": "gulp build && rollup -c"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^rakam$": "<rootDir>/main"
    }
  },
  "keywords": [
    "math",
    "mathematics",
    "rakam",
    "latex",
    "arithmetic",
    "interval",
    "complex",
    "geometry",
    "angles",
    "fraction",
    "interval-arithmetic",
    "array-arithmetic",
    "calculator",
    "numpy"
  ],
  "author": "sciCave",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/plugin-transform-exponentiation-operator": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/plugin-transform-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "jest": "^26.4.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@scicave/math-latex-parser": "^1.1.1",
    "@scicave/math-parser": "^2.2.2",
    "fraction.js": "^4.0.12"
  }
}
